{
  "name": "pyebwa-auth-server",
  "version": "1.0.0",
  "description": "Centralized authentication server for Pyebwa",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "pm2:start": "pm2 start server.js --name pyebwa-auth",
    "pm2:stop": "pm2 stop pyebwa-auth",
    "pm2:restart": "pm2 restart pyebwa-auth",
    "pm2:logs": "pm2 logs pyebwa-auth"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "nodemon": "^2.0.22"
  }
}